RewriteRule (.*|\/) index.php [QSA,L]
#RewriteCond %{REQUEST_URI} !adaptive-images\.php

RewriteCond %{REQUEST_URI}  !(\.png|\.jpg|\.gif|\.jpeg|\.js|\.css|\.ico)$
#RewriteRule (.*)  route_apache.php [QSA,L]

#RewriteRule (.*)  index.html [QSA,L]